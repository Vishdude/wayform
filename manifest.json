{
  "manifest_version": 3,
  "name": "Wayform",
  "version": "1.3.5",

  "description": "Marks moderately AI chosen answers on your google forms.",

  "icons": {
    "48": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.google.com/forms/*"],
      "js": ["content/content.js"]
    }],
  "action":{
  	"default_popup": "popup/popup.html"
  },
"permissions": 
	["scripting","activeTab","storage"],
"browser_specific_settings": {
    "gecko": {
        "id": "wayform@cool.org",
        "strict_min_version": "109.0"
    },
    "gecko_android": {
      "strict_min_version": "113.0"
    }
}

}
